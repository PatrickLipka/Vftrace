{
  "scenario_name": "TLB + L3",
  "CPU Type": "Skylake",
  "counters": [
    {
      "papi_name": "PERF_COUNT_HW_CACHE_DTLB:ACCESS",
      "symbol": "f1"
    },
    {
      "papi_name": "PERF_COUNT_HW_CACHE_DTLB:MISS",
      "symbol": "f2"
    },
    {
      "papi_name": "LONGEST_LAT_CACHE:REFERENCE",
      "symbol": "f3"
    },
    {
      "papi_name": "LONGEST_LAT_CACHE:MISS",
      "symbol": "f4"
    }
  ],
  "observables": [
    {
      "name": "TLB hit ratio",
      "formula": "f1 * 100 / (f1 + f2)",
      "default": "0",
      "format": [{
		"header": "TLB Hit",
		"unit": "%",
		"decimal_places": "1"}]
    },
    {
      "name": "Memory throughput",
      "formula": "f4 * 64 * 1.e-9 / runtime",
      "protected": "runtime",
      "default": "0",
      "format": [{
		"header": "Memory",
		"unit": "GB/s",
		"decimal_places": "3"}]
    },
    {
      "name": "L3 cache hit ratio",
      "formula": "(f3 - f4) * 100 / f3",
      "protected": "f3",
      "default": "0",
      "format": [{
		"header": "L3 Hit",
		"unit": "%",
		"decimal_places": "1"}]
    }
  ]
}

