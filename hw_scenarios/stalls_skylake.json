{
  "scenario_name": "Stalls",
  "CPU Type": "Skylake",
  "counters": [
    {
      "papi_name": "RESOURCE_STALLS:ANY",
      "symbol": "f1"
    },
    {
      "papi_name": "UNHALTED_CORE_CYCLES",
      "symbol": "f2"
    },
    {
      "papi_name": "INST_RETIRED:ANY_P",
      "symbol": "f3"
    }
  ],
  "observables": [
    {
      "name": "Stall ratio",
      "formula": "f1 * 100 / f2",
      "protected": "f2",
      "default": "0",
      "format": [{
		"header": "Stall ratio",
		"unit": "%",
		"decimal_places": "1"}]
    },
    {
      "name": "CPU clock frequency",
      "formula": "f2 * 1.e-9 / runtime",
      "protected": "runtime",
      "default": "0",
      "format": [{
		"header": "Frequency",
		"unit": "GHz",
		"decimal_places": "2"}]
    },
    {
      "name": "Clocks per instruction",
      "formula": "cycles / f3",
      "protected": "f3",
      "default": "0",
      "format": [{
		"header": "Clocks / instruction",
		"unit": "",
		"decimal_places": "2"}]
    }
  ]
}

